    <script src="https://cdn.jsdelivr.net/npm/@widgetbot/html-embed"></script>
    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const discordWidget = document.querySelector(".discord-widget");
        const discordFallback = document.querySelector(".discord-fallback");

        // Lazy load the Discord widget
        const observer = new IntersectionObserver(
          (entries) => {
            entries.forEach((entry) => {
              if (entry.isIntersecting) {
                discordWidget.style.display = "block";
                observer.unobserve(entry.target);
              }
            });
          },
          { threshold: 0.1 }
        );

        observer.observe(discordWidget);

        // Handle widget load error
        discordWidget.addEventListener("error", function () {
          discordWidget.style.display = "none";
          discordFallback.style.display = "block";
        });
      });
    </script>
